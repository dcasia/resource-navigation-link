{
    "name": "digital-creative/resource-navigation-link",
    "description": "Create links to internal or external resources.",
    "keywords": [
        "laravel",
        "nova",
        "tabs",
        "card",
        "field"
    ],
    "authors": [
        {
            "name": "Rafael Milewski"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "DigitalCreative\\ResourceNavigationLink\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalCreative\\ResourceNavigationLink\\ResourceNavigationLinkServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
